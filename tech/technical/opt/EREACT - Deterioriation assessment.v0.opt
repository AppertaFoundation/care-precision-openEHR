                                </term_definitions>
                                <term_definitions code="at0005">
                                    <items id="text">Alert</items>
                                    <items id="description">Fully awake. Spontaneous opening of the eyes, responds to voice and have motor function.</items>
                                </term_definitions>
                                <term_definitions code="at0006">
                                    <items id="text">Voice</items>
                                    <items id="description">Any verbal, motor or eye response to a voice stimulus.</items>
                                </term_definitions>
                                <term_definitions code="at0007">
                                    <items id="text">Pain</items>
                                    <items id="description">Any verbal, motor or eye response to a pain stimulus.</items>
                                </term_definitions>
                                <term_definitions code="at0008">
                                    <items id="text">Unresponsive</items>
                                    <items id="description">No response to voice or pain stimuli.</items>
                                </term_definitions>
